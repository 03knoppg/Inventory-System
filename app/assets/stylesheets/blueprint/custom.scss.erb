.container {
  width: 710px;
  background: white;
  padding: 10px;
  padding-top: 10px;
  margin-top: 15px;
}

.containerLinks {
   width:100px;
   background: teal;
   float: right;
}

body {
  background: #cff; /*url("/assets/leopardskin.gif") repeat;*/

}

header {
  padding-top: 20px;
}

header img {
  padding: 1em;
  background: #fff;
}

section {
  margin-top: 1em;
  font-size: 120%;
  padding: 20px;
  background: #fff;
}

h1 {
  font-size: 300%;
  border-bottom: 1px #333 solid;
  padding-bottom: 5px;
}

h4 {
  font-size: 150%;
  padding-top: 20px;
  margin-bottom: 5px;
}

/* Links */

a {
  color: #09c;
  text-decoration: none;
}

a:hover {
  color: #080;
  text-decoration: underline;
}

a:visited {
  color: #069;
}

.button_to {
  float: left;
  padding-right: 5px;

}

.clear {
  clear: both;
}

th{ font-size: 150%;
    border-bottom: 1px #333 solid;
    padding-bottom: 1px;
  }

 /* Navigation */

nav {
  float: right;
}

nav {
  background-color: #cff;
  padding: 0 0.7em;
  white-space: nowrap;
}

nav ul {
  margin: 0;
  padding: 0;
}

nav ul li {
  list-style-type: none;
  display: inline-block;
  padding: 0.2em 0;
}

nav ul li a {
  padding: 0 5px;
  font-weight: bold;
}

nav ul li a:visited {
  color: red;
}

nav ul li a:hover {
  text-decoration: underline;
}
