.container {
  width: 1200px;
  background: white ;
  padding: 60px;
  padding-top: 20px;
  padding-bottom: 20px;
  margin-top: 15px;
  margin-bottom: 15px;
}

.containerBanner{
    width:1200px;
    height: 100px;
    background: white;
    padding: 0px;
    padding-top: 0px;
    padding-bottom: 0px;
    margin-top: 10px;
    margin-bottom: 20px;
}

.containerLinks {
   width:100px;
   background: teal;
   float: right;
}

body {
  background: #cff;
}

header {
  padding-top: 20px;
}

header img {
  padding: 1em;
  background: #fff;
}

section {
  margin-top: 1em;
  font-size: 120%;
  padding: 20px;
  background: #fff;
}

/* Headers */
h1 {
  font-size: 300%;
  border-bottom: 1px #333 solid;
  padding-bottom: 5px;
}

h2 {
    font-size: 175%;
    font-weight: bold;
    border-bottom: 1px #333 solid;
    margin-bottom: 5px;
}

h4 {
  font-size: 125%;
  padding-top: 20px;
  margin-bottom: 5px;
}

h5{
    font-size: 100%;
    font-weight:bold;
}

/* Div */

.main_div {
  padding:20px;
  border-width:3px;
  border-spacing:2px;
  border-style:solid;
}
.right_div {
  float:right;
  padding-top:20px;
}

.new_button_div{
    padding-top:20px;
    padding-bottom:20px;
}
/* Buttons */

.button_to {
  float: left;
  padding-right: 5px;

}

/* Clear */
.clear {
  clear: both;
}

/*Tables */

th{ font-size: 25px;
    border-bottom: 1px #333 solid;
    padding-bottom: 1px;
    font-weight:normal;
}
.main_table  {
    border-width:5px;
    border-spacing:2px;
    border-style:ridge;
}
.inner_table {
    border-width:2px;
    border-spacing:2px;
    border-style:double;
}

td{
  font-family:Tahoma, Geneva, sans-serif;
  font-size:15px;
  vertical-align: top;
}

/* Navigation */

nav {
  float: right;
}

nav {
  background-color: #cff;
  padding: 0 0.7em;
  white-space: nowrap;
}

nav ul {
  margin: 0;
  padding: 0;
}

nav ul li {
  list-style-type: none;
  display: inline-block;
  padding: 0.2em 0;
}

nav ul li a {
  padding: 0 5px;
  font-weight: bold;
}

nav ul li a:visited {
  color: blue;
}

nav ul li a:hover {
  text-decoration: underline;
}
