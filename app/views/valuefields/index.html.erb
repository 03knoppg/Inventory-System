<h1>Listing valuefields</h1>

<table>
  <tr>
    <th>Fieldvalue</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @all_atrributes.each do |att| %>
      <tr>
        <td>
          <b><%=att.name%></b>
        </td>
      </tr>
      <%for val in att.valuefields %>
      <tr>
        <td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%= val.fieldvalue %></td>
        <td><%= link_to 'Show', val %></td>
        <td><%= link_to 'Edit', edit_valuefield_path(val) %></td>
        <td><%= link_to 'Destroy', val, confirm: 'Are you sure?', method: :delete %></td>
      </tr>
    <% end %>
<% end %>
</table>

<br />

<%= link_to 'New Valuefield', new_valuefield_path %>
