<!--Button to create a new property -->
<%= button_to "New #{@property_alias}", new_property_path, :method => :get %> <br /><br /><br />
<!--Properties Index Page - uses application.html.erb for template. -->
<h1>Listing <%= @property_alias %></h1>

<div class="main_div">
 <!--if length > 0 print properties -->
<%if(@all_properties.length > 0)%>
    <table>
        <tr><th colspan="5"><h4>Current <%= @property_alias %></h4></th></tr>
        <tr>
            <th align="left">Name</th>
            <th align="left">Description</th>
            <th align="left">Field Type</th>
            <th align="left">Actions</th>
        </tr>
        <%= print_properties.html_safe %>
<%else%>
        <tr><td><h4>No <%= @property_alias %> Available</h4></td></tr>
<%end%>
</table>
</div>
 <br class="clear" />