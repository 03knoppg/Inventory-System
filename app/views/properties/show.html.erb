 <p id="notice"><%= notice %></p>
 <h1>Viewing Property</h1>
 <%= button_to "Edit Property", edit_property_path(@property), :method => :get %>
<table>
  <tr>
    <th align="left">Name</th>
    <th align="left">Description</th>
    <th align="left">field type</th>
  </tr>
  <tr>
    <td><%= @property.name %></td>
    <td><%= @property.description %></td>
    <td><%= @property.field_type %></td>
  </tr>
</table>

<h4> Value Fields related to this Group </h4>
<% vf = @property.valuefields %>
<table>
  <tr><th>Name</th></tr>
  <% for v in vf %>
    <tr><td><%= link_to(v.fieldvalue, v) %></td></tr>
 <%end%>
</table>
<br class="clear" />
