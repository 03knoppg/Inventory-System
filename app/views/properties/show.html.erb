 <p id="notice"><%# notice %></p>
 <h1>Viewing Property: <%=@property.name%></h1>
 <div class="main_div">
<table class="main_table">
  <tr>
    <th align="left">Name</th>
    <th align="left">Description</th>
    <th align="left">field type</th>
  </tr>
  <tr>
    <td><%= @property.name %></td>
    <td><%= @property.description %></td>
    <td><%= @property.field_type %></td>
  </tr>
</table>

   <!-- the tabs -->
<ul class="tabs">
	<li><a href="#">Associations</a></li>
</ul>
<!-- tab "panes" -->
<div class="panes">
  <div>
    <%= print_valuefields_table(@property).html_safe%>
   </div>
  </div>
   </div>

   <div class="right_div">
     <%= button_to "Edit Property", edit_property_path(@property), :method => :get %>
     <%=button_to "Delete Property", {:controller => :properties, :action => "destroy", :id => @property.id }, :confirm => "Are You sure?", :method => :delete%>
   </div>
<br class="clear" />

 <script type="text/javascript">

    jQuery(function($) {
         $('#submit_button').click(function () {
            $.get("/valuefields/new", {property_id: "<%=@property.id%>"}, null);
            return false;
        });
    });
 </script>


