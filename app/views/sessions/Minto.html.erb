
<button type="button" disabled="false">Submit</button>

<Table>

          <tr><th>Block</th><th>Unit Type</th><th>Number</th><th>Price</th><th>Available</th><th>Premium</th></tr>

<%i = 0
for s in Category.all

       if(s.name == "Minto")
   for suite in s.products
    if( i % 5 == 0)%>

    <%end

        i = i + 1%>

              <tr>
                   <td>
                     <%= suite.name[1] %>
                   </td>
                   <td>
                     <%= suite.name[3..4] %>
                   </td>
                   <td>
                     <%= suite.name[9..10] %>
                   </td>
                   <td>
                     <%= text_field_tag "Text", suite.valuefields[1].fieldvalue.to_s, :id=>suite.name%>
                   </td>
                   <td>
                     <%= check_box_tag "CheckBox", suite.name, suite.valuefields[0].fieldvalue == "true" ? true : false, :id=>suite.name%>
                   </td>
                   <td>
                      <%= text_field_tag "Premium", suite.valuefields[3].fieldvalue.to_s, :id=>suite.name%>
                   </td>


              </tr>

        <%end
        end%>
<%end%>

</Table>
<script type="text/javascript">
    jQuery(function($) {


        $('#CheckBox').click(function(){
            $.get("/sessions/update_minto", {id: $(this)[0].id, checked: $(this)[0].checked}, null, 'script');
           $('button').removeAttr("disabled");

        });

        $('input[name="Text"]').change(function(){
           $.get("/sessions/update_minto_price", {id: $(this)[0].id, price: $(this)[0].value}, null, 'script');

        });

        $('input[name="Premium"]').change(function(){
           $.get("/sessions/update_minto_premium", {id: $(this)[0].id, premium: $(this)[0].value}, null, 'script');

        });

        $('input').click(function(){
           $('button').removeAttr("disabled");

        });

        $('button').click(function(){
            $(this)[0].disabled = "true";
        });
    });


</script>